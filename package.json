{
  "name": "react-timelines-modern",
  "version": "1.0.0",
  "description": "React Timelines: Modern Implementation",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib coverage",
    "build:js": "tsc --project tsconfig.build.json",
    "watch:js": "tsc --project tsconfig.build.json --watch",
    "build:css": "node-sass src/scss/style.scss lib/css/style.css",
    "watch:css": "node-sass -w src/scss/style.scss lib/css/style.css",
    "watch": "npm run watch:js & npm run watch:css",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "test": "npm run lint && npm run unit",
    "lint": "npm run lint:prettier && npm run lint:js",
    "lint:js": "eslint . --ext .ts,.tsx",
    "lint:js:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --list-different \"{e2e,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier:fix": "prettier --write \"{e2e,src}/**/*.{js,jsx,ts,tsx}\"",
    "unit": "jest",
    "coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx,ts,tsx}'",
    "prepublish": "npm run clean && npm run build",
    "demo:deploy": "./demo/ops/deploy.sh"
  },
  "keywords": [
    "timeline",
    "schedule",
    "history",
    "react",
    "gantt",
    "horizontal",
    "library",
    "scroll",
    "scss",
    "sass",
    "tracks",
    "time"
  ],
  "repository": "git@github.com:JSainsburyPLC/react-timelines.git",
  "author": "JSainsburyPLC",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.14.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.2.0",
    "@types/react": "^16.14.32",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.31.10",
    "jest": "^29.2.1",
    "node-sass": "^7.0.3",
    "prettier": "^2.0.5",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "jest": {
    "rootDir": "./src",
    "resetMocks": true,
    "resetModules": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/utils/raf.js",
      "<rootDir>/utils/events.js",
      "<rootDir>/utils/computedStyle.js",
      "<rootDir>/propTypes.js"
    ],
    "setupFiles": [
      "../jestSetup.js"
    ]
  }
}
